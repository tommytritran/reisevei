{
  "manifest_version": 3,
  "name": "Reisevei",
  "version": "1.0.0",
  "description": "Finn reisevei",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Reisevei"
  },
  "icons": {
    "16": "images/commute_16.png",
    "32": "images/commute_32.png",
    "128": "images/commute_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.finn.no/realestate/homes/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "permissions": ["storage"]
}
